{
  "name": "fewsats",
  "version": "0.1.4",
  "description": "TypeScript client for Fewsats API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
